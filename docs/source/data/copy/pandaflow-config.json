{
    "data_sources": [
        {
            "type": "csv",
            "path": "./input.csv",
            "parse_dates": ["date"]
        }
    ],
    "rules": [
        {
            "field": "__total__",
            "strategy": "copy",
            "source": "Amount",
            "input_rule": "default_currency",
            "output_rule": "float_2dec",
            "fillna": 0
        }
    ]
}