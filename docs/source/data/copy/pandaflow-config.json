{
    "data_sources": [
        {
            "type": "csv",
            "path": "./input.csv",
            "parse_dates": ["date"]
        }
    ],
    "transformations": [
        {
            "field": "__total__",
            "strategy": "copy",
            "source": "Amount",
            "parser": "default_currency",
            "formatter": "float_2dec",
            "fillna": 0
        }
    ]
}